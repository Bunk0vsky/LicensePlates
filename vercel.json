{
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=59"
        }
      ]
    }
  ],

  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "postbuild": "npm run minify"
  }
}
